sName		star		# Body's name
saModules stellar eqtide

# Physical Parameters
dMass		0.12             # Mass, negative -> Earth masses
dRadGyra	0.5		# Radius of gyration (moment of inertia constant)
dRotPeriod      -83.5

# EQTIDE Parameters
#dTidalTau	-1		# Tidal time lag, negative -> seconds
dTidalQ		1.0e6		# Tidal phase lag
dK2		0.5		# Love number of degree 2
sTideModel	p2		# Tidal model, p2=CPL, t8=CTL
saTidePerts	pandora		# Body name(s) of tidal perturbers

# STELLAR parameters
sStellarModel proximacen              # Stellar evolutin model: `baraffe` or `none`
sWindModel    reiners                    # Stellar wind model: `reiners` or `none`
dSatXUVFrac   1.e-3                       # Saturation level of the XUV luminosity

saOutputOrder	Time #-Radius -RotRate -Luminosity
